<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>ofertas</from-outcome>
            <to-view-id>/ofertas/pasantias_vigentes.xhtml</to-view-id>
            <redirect>1</redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>datospersonales</from-outcome>
            <to-view-id>/datospersonales/datospersonales.xhtml</to-view-id>
            <redirect>1</redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>reglamentacion</from-outcome>
            <to-view-id>/reglamentacion/reglamentacion.xhtml</to-view-id>
            <redirect>1</redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>pasantiasencurso</from-outcome>
            <to-view-id>/pasantias/pasantiasencurso.xhtml</to-view-id>
            <redirect>1</redirect>
        </navigation-case>
    </navigation-rule>
</faces-config>
